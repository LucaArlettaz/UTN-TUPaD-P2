public class Principal {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Paciente paciente = new Paciente("Maria Lopez", "OSDE");
        Profesional profesional = new Profesional("Dr. Juan Gomez", "Cardiologia");

        CitaMedica cita = new CitaMedica("25/10/2025", "10:30", paciente, profesional);
        cita.mostrarInfo();
    }
}
